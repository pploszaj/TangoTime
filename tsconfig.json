{
    "compilerOptions": {
      "jsx": "react",
      "target": "es6",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "outDir": "./dist"
    },
    "include": ["src/**/*"]
}